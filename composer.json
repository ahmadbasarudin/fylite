{
    "name": "biobii/fylite",
    "description": "FYLite is mini MVC framework for small application. Lite, simple and easy to use.",
    "keywords": ["mvc", "framework", "lite"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Biobii",
            "email": "biobii.game@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "FYLite\\": "system/"
        },
        "files": [
            "system/helpers/helper.php"
        ]
    },
    "require": {
        "php": ">=5.6.0",
        "nikic/fast-route": "^1.3",
        "slim/pdo": "~1.10",
        "jenssegers/blade": "^1.1",
        "vlucas/phpdotenv": "^2.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/biobii/fylite"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
